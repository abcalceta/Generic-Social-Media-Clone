<% if user_signed_in? %>
  <% if current_user.has_favorited?(@post) %>
    <%= link_to "Unfavorite", post_favorite_path(@post), method: :delete, remote: true %>
  <% else %>
    <%= link_to "Favorite", post_favorite_path(@post), method: :post, remote: true %>
  <% end %>
<% end %>

<% @post.favorites.each do |fav| %>
  <%= link_to fav.user_id, User.find(fav.user_id)%>
<% end %>
